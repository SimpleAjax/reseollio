{
    "name": "reseolio",
    "version": "0.1.1",
    "description": "The Durable Execution for Modern Backends - Node.js SDK",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts",
        "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
        "debug": "tsx test-debug.ts",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:e2e": "tsx tests/e2e/run-all.ts",
        "test:e2e:restart": "node tests/e2e/restart-test.mjs",
        "test:load": "tsx tests/load/run-all.ts",
        "test:all": "npm run test:e2e && npm run test:load",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src --ext .ts"
    },
    "keywords": [
        "durable",
        "execution",
        "retry",
        "queue",
        "job",
        "background",
        "task",
        "resilient"
    ],
    "author": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/reseolio/reseolio.git",
        "directory": "sdks/node"
    },
    "files": [
        "dist",
        "bin",
        "vendor"
    ],
    "bin": {
        "reseolio": "./bin/reseolio.js"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.9.13",
        "@grpc/proto-loader": "^0.7.10",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/exporter-trace-otlp-grpc": "^0.209.0",
        "@opentelemetry/instrumentation-grpc": "^0.209.0",
        "@opentelemetry/resources": "^2.3.0",
        "@opentelemetry/sdk-node": "^0.209.0",
        "@opentelemetry/semantic-conventions": "^1.38.0",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/node": "^20.10.5",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "tsup": "^8.0.1",
        "tsx": "^4.21.0",
        "typescript": "^5.3.3",
        "vitest": "^1.1.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}